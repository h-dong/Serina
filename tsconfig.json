{
    "version": "1.5.0",
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "declaration": false,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "removeComments": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "typeRoots": [
            "/.node_modules/@types"
        ],
        "types": [
            "jest"
        ]
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
